{
    "sourceRoot": "modules/react-components/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nrwl/web:rollup",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "modules/react-components/dist",
                "tsConfig": "modules/react-components/tsconfig.lib.json",
                "project": "modules/react-components/package.json",
                "entryFile": "modules/react-components/src/index.ts",
                "external": [
                    "react/jsx-runtime"
                ],
                "rollupConfig": "modules/react-components/rollup.config.cjs",
                "compiler": "babel",
                "assets": [
                    {
                        "glob": "modules/react-components/README.md",
                        "input": ".",
                        "output": "."
                    }
                ]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "modules/react-components/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/modules/react-components"
            ],
            "options": {
                "jestConfig": "modules/react-components/jest.config.js",
                "passWithNoTests": true
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/react",
                "port": 6006,
                "config": {
                    "configFolder": "modules/react-components/.storybook",
                    "pluginPath": "modules/react-components/.storybook/plugins.js",
                    "configPath": "modules/react-components/.storybook/preview.js",
                    "srcRoot": "modules/react-components/src"
                },
                "docsMode": true,
                "quiet": false
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "uiFramework": "@storybook/react",
                "outputPath": "dist/storybook/react-components",
                "config": {
                    "configFolder": "modules/react-components/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    }
}
