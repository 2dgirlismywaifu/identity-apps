{
    "name": "@wso2is/user-portal",
    "version": "1.0.304",
    "description": "WSO2 Identity Server User Portal",
    "author": "WSO2",
    "license": "Apache-2.0",
    "main": "index.js",
    "scripts": {
        "build": "run-script-os",
        "build:win32": "npm run webpack -- -p --env.NODE_ENV=production && npm run test",
        "build:default": "npm run webpack -- -p --env.NODE_ENV=production && npm run test",
        "build:analyze": "run-script-os",
        "build:analyze:win32": "npm run webpack -- -p --env.NODE_ENV=production --env.ENABLE_ANALYZER=true",
        "build:analyze:default": "npm run webpack -- -p --env.NODE_ENV=production --env.ENABLE_ANALYZER=true",
        "clean": "rm -rf build",
        "clean:themes": "npm run rimraf src/themes",
        "copy:themes": "node scripts/copy-themes.js",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
        "postinstall": "node scripts/post-install.js",
        "prebuild": "npm run clean",
        "rimraf": "run-script-os",
        "rimraf:default": "../../node_modules/rimraf/bin.js",
        "rimraf:win32": "..\\..\\node_modules\\rimraf\\bin.js",
        "start": "run-script-os",
        "start:win32": "npm run webpack-dev-server -- --env.NODE_ENV=development --inline --hot --open",
        "start:default": "npm run webpack-dev-server -- --env.NODE_ENV=development --inline --hot --open",
        "test": "jest --passWithNoTests",
        "webpack": "run-script-os",
        "webpack:default": "../../node_modules/.bin/webpack",
        "webpack:win32": "..\\..\\node_modules\\.bin\\webpack",
        "webpack-dev-server": "run-script-os",
        "webpack-dev-server:default": "../../node_modules/.bin/webpack-dev-server",
        "webpack-dev-server:win32": "..\\..\\node_modules\\.bin\\webpack-dev-server"
    },
    "dependencies": {
        "@wso2is/authentication": "^1.0.304",
        "@wso2is/core": "^1.0.304",
        "@wso2is/forms": "^1.0.304",
        "@wso2is/http": "^1.0.304",
        "@wso2is/i18n": "^1.0.304",
        "@wso2is/oauth-web-worker": "^1.0.304",
        "@wso2is/react-components": "^1.0.304",
        "@wso2is/theme": "^1.0.304",
        "@wso2is/validation": "^1.0.304",
        "qrcode.react": "^1.0.0"
    },
    "devDependencies": {
        "run-script-os": "^1.0.7"
    },
    "peerDependencies": {
        "axios": "*",
        "fork-ts-checker-webpack-plugin": "*",
        "history": "*",
        "i18next": "*",
        "i18next-browser-languagedetector": "*",
        "lodash": "*",
        "log": "*",
        "moment": "*",
        "react-i18next": "*",
        "react-notification-system": "*",
        "react-redux": "*",
        "react-router-dom": "*",
        "redux": "*",
        "redux-devtools-extension": "*",
        "redux-thunk": "*",
        "thread-loader": "*",
        "ua-parser-js": "*"
    }
}
