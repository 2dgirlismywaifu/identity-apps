{
  "version": 2,
  "projects": {
    "access-control": {
      "root": "modules/access-control",
      "projectType": "library"
    },
    "authentication-portal": {
      "root": "apps/authentication-portal",
      "projectType": "application"
    },
    "console": {
      "projectType": "application",
      "generators": {},
      "root": "apps/console",
      "sourceRoot": "apps/console/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "apps/build/console"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "apps/console"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "core": {
      "projectType": "library",
      "generators": {},
      "root": "modules/core",
      "sourceRoot": "modules/core/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/core/dist"
          ],
          "options": {
            "tsconfig": "modules/core/tsconfig.json",
            "command": "npm run compile",
            "cwd": "modules/core"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "form": {
      "projectType": "library",
      "generators": {},
      "root": "modules/form",
      "sourceRoot": "modules/form/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/form/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/form"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "forms": {
      "projectType": "library",
      "generators": {},
      "root": "modules/forms",
      "sourceRoot": "modules/forms/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/forms/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/forms"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "i18n": {
      "root": "modules/i18n",
      "projectType": "library"
    },
    "myaccount": {
      "root": "apps/myaccount",
      "sourceRoot": "apps/myaccount/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "compiler": "babel",
            "outputPath": "apps/myaccount/build/myaccount",
            "index": "apps/myaccount/src/index.html",
            "baseHref": "",
            "main": "apps/myaccount/src/index.tsx",
            "polyfills": "",
            "tsConfig": "apps/myaccount/tsconfig.json",
            "assets": [
              {
                "glob": "**/*.!(less)",
                "input": "modules/theme/dist/lib",
                "output": "libs"
              },
              {
                "glob": "**/*",
                "input": "apps/myaccount/src/public",
                "output": "."
              },
              {
                "glob": "**/*",
                "input": "modules/i18n/dist/bundle",
                "output": "resources/i18n"
              },
              {
                "glob": "**/assets/images/(branding|flags.png)/**",
                "input": "modules/theme/dist/lib",
                "output": "libs"
              }
            ],
            "styles": [],
            "scripts": [],
            "generateIndexHtml": false,
            "webpackConfig": "apps/myaccount/nx-webpack.ts"
          },
          "configurations": {
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "fileReplacements": [],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "myaccount:build",
            "port": 9000,
            "baseHref": "",
            "open": true,
            "hmr": true,
            "ssl": true
          },
          "configurations": {
            "development": {
              "buildTarget": "myaccount:build:development"
            },
            "production": {
              "buildTarget": "myaccount:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/myaccount/**/*.{ts,tsx,js,jsx}"
            ],
            "eslintConfig": "apps/myaccount/.eslintrc.js"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/myaccount"
          ],
          "options": {
            "jestConfig": "apps/myaccount/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "react-components": {
      "projectType": "library",
      "generators": {},
      "root": "modules/react-components",
      "sourceRoot": "modules/react-components/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/react-components/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/react-components"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "recovery-portal": {
      "root": "apps/recovery-portal",
      "projectType": "application"
    },
    "sms-otp-authentication-portal": {
      "root": "apps/sms-otp-authentication-portal",
      "projectType": "application"
    },
    "tests": {
      "root": "tests",
      "projectType": "library"
    },
    "theme": {
      "root": "modules/theme",
      "projectType": "library"
    },
    "theme-component": {
      "root": "components/theme-component",
      "projectType": "library"
    },
    "validation": {
      "root": "modules/validation",
      "projectType": "library"
    },
    "x509-certificate-authentication-portal": {
      "root": "apps/x509-certificate-authentication-portal",
      "projectType": "application"
    }
  }
}
